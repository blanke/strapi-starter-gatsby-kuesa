			},
			breakpoints:[576, 768, 1321]
		});

		function resizePlaceholders(){
			var element = document.getElementsByClassName('outerYoutubePlaceholder');

			var i;
			for (i = 0; i < element.length; i++) {
			  var width = window.getComputedStyle(element[i]).getPropertyValue('width');
			  element[i].style.height = width.substring(0, width.length -2)*0.565 + "px";
			} 
			
		}

	</script>

