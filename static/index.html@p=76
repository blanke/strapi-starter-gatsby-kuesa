			//Menu bar
			jQuery(function($) {
				var modalBlock = $('.modal');
				$(window).scroll(function() {
					if ( $(this).scrollTop() > 0 ) {
						modalBlock.addClass('scrolledPadding');
					} else {
						modalBlock.removeClass('scrolledPadding');
					}
				});
			});

	function  pageSetup(){

		var test = new Rellax('.parallaxImage', {
			callback: function (position) {
			//console.log(position);
			},
		breakpoints:[576, 768, 1280]
		});

		resizePlaceholders()

		}
		var rellax = new Rellax('.parallax', {
			// center: true
			callback: function (position) {
			}
		});
		var rellax = new Rellax('.backgroundParallax', {
				// center: true
